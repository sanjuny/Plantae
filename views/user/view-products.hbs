<main class="main-content">
    <div class="breadcrumb-area breadcrumb-height" data-bg-image="/user/images/breadcrumb/bg/1-1-1919x388.jpg">
        <div class="container h-100">
            <div class="row h-100">
                <div class="col-lg-12">
                    <div class="breadcrumb-item">
                        <h2 class="breadcrumb-heading">View products</h2>
                        <ul>
                            <li>
                                <a href="/">Home</a>
                            </li>
                            <li>My Account</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="product-area section-space-top-100">
        <div class="container">
            <div class="section-title-wrap">
                <h2 class="section-title mb-0">Orederd Products</h2>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="tab-content" id="myTabContent">
                        <div class="tab-pane fade show active" id="featured" role="tabpanel"
                            aria-labelledby="featured-tab">
                            <div class="product-item-wrap row">
                                {{#each orderItems}}
                                <div class="col-xl-3 col-md-4 col-sm-6 mb-5 mt-5 card">
                                    <div class="product-item">
                                        <div class="product-img">
                                            {{!-- <a href=""> --}}
                                                <img class="primary-img" style="width:270px ; height:300px"
                                                    src="/admin/images/{{this.product.image.[0]}}" alt="Product Images">
                                                <img class="secondary-img" style="width:270px ; height:300px"
                                                    src="/admin/images/{{this.product.image.[1]}}" alt="Product Images">
                                                {{!-- </a> --}}
                                        </div>
                                        <div class="product-content">
                                            <a class="product-name">{{this.product.Name}}</a>
                                            <div class="price-box pb-1">
                                                quantity: <span class="new-price"></span>{{this.quantity}}</span>
                                            </div>
                                            <div class="price-box pb-1">
                                                <span class="new-price">â‚¹{{this.product.Price}}</span>
                                            </div>
                                            <div class="price-box pb-1">
                                                <span class="new-price">{{this.Description}}</span>
                                            </div>
                                            <div class="rating-box">
                                                <ul>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


</main>