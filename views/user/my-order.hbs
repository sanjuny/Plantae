<main class="main-content">
    <div class="breadcrumb-area breadcrumb-height" data-bg-image="/user/images/breadcrumb/bg/1-1-1919x388.jpg">
        <div class="container h-100">
            <div class="row h-100">
                <div class="col-lg-12">
                    <div class="breadcrumb-item">
                        <h2 class="breadcrumb-heading">My Account Page</h2>
                        <ul>
                            <li>
                                <a href="/">Home</a>
                            </li>
                            <li>My Account</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container  py-5">
        <div class="section-title-wrap mb-5">
            <h2 class="section-title mb-0">Orederd Products</h2>
        </div>
        <table class="table table-striped table-hover table-bordered">
            <thead>
                <tr>
                    <th scope="col">S.no</th>
                    <th scope="col">Date</th>
                    <th scope="col">Address</th>
                    <th scope="col">Pincode</th>
                    <th scope="col">Mobile</th>
                    <th scope="col">Amount</th>
                    <th scope="col">Payment</th>
                    <th scope="col">Status</th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                {{#each orders}}
                <tr>
                    <th scope="row">{{@index}}</th>
                    <td>{{this.date}}</td>
                    <td>{{this.deliveryDetails.Address}}</td>
                    <td>{{this.deliveryDetails.Postcode}}</td>
                    <td>{{this.deliveryDetails.Mobile}}</td>
                    <td>â‚¹{{this.TotalAmount}}</td>
                    <td>{{this.paymentMethod}}</td>
                    <td>{{this.status}}</td>
                    <td>
                        <a href="/view-products/{{this._id}}"
                            class="btn btn-outline-success btn-round waves-effect waves-light" style="width:140px ;"><i
                                class="mdi mdi-send mr-2"></i>View Products</a>
                        <a href="/cancel-order/{{this._id}}"
                            class="btn btn-outline-danger btn-round waves-effect waves-light mt-2" style="width:140px"
                            onclick="return confirm('Are you want to cancel?')"><i class="mdi mdi-power mr-2"></i>Cancel
                            Order</a>
                    </td>
                </tr>
                {{/each}}
            </tbody>

        </table>
    </div>
</main>
