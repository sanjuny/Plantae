<div class="content">


    <div class="page-content-wrapper ">

        <div class="container-fluid">
            <div class="row">
                <div class="col-12 mt-3">
                    <div class="card">
                        <div class="card-body">

                            <h3>Daily Report</h3>
                            <table id="datatable-buttons"
                                class="table table-striped table-bordered dt-responsive nowrap"
                                style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>DATE</th>
                                        <th>ORDER ID</th>
                                        <th>PAYMENT METHOD</th>
                                        <th>TOTAL REVENUE</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each category}}
                                    <tr>
                                        <td>{{@index}}</td>

                                        <td>{{this.category}}</td>
                                        <td>{{this._id}}</td>
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div> <!-- end col -->
            </div> <!-- end row -->

        </div><!-- container -->

    </div> <!-- Page content Wrapper -->
</div> <!-- content -->